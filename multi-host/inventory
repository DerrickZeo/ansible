all:
  hosts:
    web01:
      ansible_host: 172.31.16.139
      ansible_user: ec2-user
      ansible_ssh_private_key_file: client-key.pem
    web02:
      ansible_host: 172.31.29.88
      ansible_user: ec2-user
      ansible_ssh_private_key_file: client-key.pem
    db01:
      ansible_host: 172.31.25.118
      ansible_user: ec2-user
      ansible_ssh_private_key_file: client-key.pem

#The following are groups

  children:
    webservers:
      hosts:
        web01:
        web02:
    dbservers:
      hosts:
        db01:

#The following is a group of groups

    prod_stack:
      children:
        webservers:
        dbservers:
